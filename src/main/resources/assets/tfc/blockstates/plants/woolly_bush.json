{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": {
        "model": "tfc:plants/woolly_bush_shortest"
      }
    },
    {
      "when": {
        "age": "1"
      },
      "apply": {
        "model": "tfc:plants/woolly_bush_shorter"
      }
    },
    {
      "when": {
        "age": "2"
      },
      "apply": [
        {
          "model": "tfc:plants/woolly_bush_short",
          "weight": 1
        },
        {
          "model": "tfc:plants/woolly_bush_big_short",
          "weight": 2
        }
      ]
    },
    {
      "when": {
        "age": "3"
      },
      "apply": [
        {
          "model": "tfc:plants/woolly_bush",
          "weight": 1
        },
        {
          "model": "tfc:plants/woolly_bush_big",
          "weight": 3
        }
      ]
    }
  ]
}