{
  "multipart": [
    {
      "when": {
        "part": "lower"
      },
      "apply": [
        {
          "model": "tfc:plants/small_red_bamboo"
        }
      ]
    },
    {
      "when": {
        "part": "middle"
      },
      "apply": [
        {
          "model": "tfc:plants/small_red_bamboo_small_leaves"
        }
      ]
    },
    {
      "when": {
        "part": "upper"
      },
      "apply": [
        {
          "model": "tfc:plants/small_red_bamboo_top",
          "y": 180
        }
      ]
    }
  ]
}
