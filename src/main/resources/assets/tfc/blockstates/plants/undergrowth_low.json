{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": {
		"model": "tfc:plants/undergrowth_low_shortest"
      }
    },
    {
      "when": {
        "age": "1"
      },
      "apply": {
		"model": "tfc:plants/undergrowth_low_shorter"
      }
    },
    {
      "when": {
        "age": "2"
      },
      "apply": {
		"model": "tfc:plants/undergrowth_low_short"
      }
    },
    {
      "when": {
        "age": "3"
      },
      "apply": {
		"model": "tfc:plants/undergrowth_low"
      }
    }
  ]
}